<!DOCTYPE html>
<html>
<head>
  <title>Squarebean</title>
  <%= stylesheet_link_tag    "application", :media => "all" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>
<body>

  <% if flash[:notice] %>
    <%= flash[:notice] %>
  <% end %>

  <% if flash[:error] %>
    <%= flash[:error] %>
  <% end %>

  <%= yield %>

</body>
</html>
